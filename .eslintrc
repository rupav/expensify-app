{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "rules": {
        "comma-dangle": "off",
        "no-console": 0,
        "linebreak-style": ["error","windows"],
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "arrow-parens": 0,
        "indent": ["error", 4],
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/prop-types": 0,
        "arrow-body-style": 0
    }
    
}
